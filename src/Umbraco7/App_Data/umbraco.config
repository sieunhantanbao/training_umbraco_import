      <TextPage id="1070" key="193013fc-6ada-4c57-8213-a7cfc260c2da" parentID="1068" level="3" creatorID="0" sortOrder="1" createDate="2016-12-03T18:31:42" updateDate="2016-12-03T18:31:59" nodeName="Codegarden" urlName="codegarden" path="-1,1063,1068,1070" isDoc="" nodeType="1062" creatorName="Umbraco7Training" writerName="Umbraco7Training" writerID="0" template="1057" nodeTypeAlias="TextPage">
        <content><![CDATA[{
  "name": "1 column layout",
